<?xml version='1.1' encoding='UTF-8'?>
<jenkins.plugins.logstash.LogstashConfiguration plugin="logstash@2.1.0">
  <logstashIndexer class="jenkins.plugins.logstash.configuration.Logstash">
    <host>logstash</host>
    <port>5044</port>
  </logstashIndexer>
  <enabled>true</enabled>
  <dataMigrated>true</dataMigrated>
  <enableGlobally>true</enableGlobally>
  <milliSecondTimestamps>true</milliSecondTimestamps>
</jenkins.plugins.logstash.LogstashConfiguration>